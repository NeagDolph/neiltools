{
	"name": "PeaceBoxWebsite",
	"version": "1.0.0",
	"scripts": {
		"dev": "source $HOME/.nvm/nvm.sh; nvm use 16; svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
      "@playwright/test": "^1.22.2",
      "@sveltejs/adapter-auto": "next",
      "@sveltejs/kit": "next",
      "autoprefixer": "^10.4.7",
      "lodash": "^4.17.21",
      "postcss": "^8.4.14",
      "prettier": "^2.6.2",
      "prettier-plugin-svelte": "^2.7.0",
      "snapsvg-cjs": "^0.0.6",
      "svelte": "^3.44.0",
      "svelte-adapter-firebase": "^0.14.1",
      "svelte-check": "^2.7.1",
      "svelte-copy": "^1.2.4",
      "svelte-preprocess": "^4.10.7",
      "svelte-preprocess-import-assets": "^0.1.6",
      "tailwindcss": "^3.1.4",
      "three": "^0.142.0",
      "threlte": "^3.13.1",
      "troika-three-text": "^0.48.0-unicode.1",
      "typescript": "^4.7.2"
    },
	"type": "module",
	"dependencies": {
      "@beyonk/svelte-facebook-pixel": "^3.0.1"
	}
}
